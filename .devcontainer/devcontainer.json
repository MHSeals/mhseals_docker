{
  "name": "Roboboat Development Container",
  "dockerComposeFile": [
    "docker-compose.yml",
    "docker-compose.override.yml"
  ],
  "service": "roboboat",
  "workspaceFolder": "/home/${env:USER}/roboboat_ws",
  "postCreateCommand": ".devcontainer/postcreate.sh",
  "remoteUser": "${env:USER}",
  "overrideCommand": true,
  "customizations": {
    "vscode": {
      "extensions":[
        "ms-vscode.cpptools",
        "ms-vscode.cpptools-themes",
        "twxs.cmake",
        "donjayamanne.python-extension-pack",
        "eamodio.gitlens",
        "Ranch-Hand-Robotics.rde-pack"
      ]
    }
  },
  "forwardPorts": [10000]
}